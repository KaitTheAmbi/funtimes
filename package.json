{
  "name": "vue-starter-express",
  "version": "0.0.1",
  "description": "Starter template for Vue.js projects with express backend.",
  "main": "server.js",
  "scripts": {
    "start": "npm run compile && node server.js",
    "compile": "browserify -t vueify -e src/main.js -o public/build.js"
  },
  "engines": {
    "node": "11.x"
  },
  "dependencies": {
    "express": "^4.16.4",
    "socket.io-client": "^2.3.0",
    "socket.io": "^2.3.0",
    "vue-socket.io": "^3.0.10",
    "browserify": "^16.2.3",
    "vueify": "^9.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-core": "^6.26.3",
    "vue": "^2.6.8",
    "vuex": "^3.5.1",
    "vue-router": "^3.3.4",
    "express-routing": "^0.0.2",
    "vuelidate": "^0.7.5",
    "node-sass": "^4.14.1",
    "sass-loader": "^9.0.3",
    "vue-color": "^2.7.1",
    "discord.js": "^12.3.1"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "glitch",
    "express",
    "vue"
  ]
}